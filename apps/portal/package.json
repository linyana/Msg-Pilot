{
  "name": "@msg-pilot/portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --mode mock",
    "start:dev": "vite --mode localdev",
    "dev:staging": "vite --mode staging",
    "dev:prod": "vite --mode prod",
    "dev": "vite --mode dev",
    "build": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --emptyOutDir",
    "build:dev": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --mode dev --emptyOutDir",
    "build:staging": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --mode staging --emptyOutDir",
    "build:main": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --mode main --emptyOutDir",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "antd": "5.24.1",
    "dayjs": "^1.11.7",
    "i18next": "^24.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "redux": "^4.2.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@types/mime": "3.0.4",
    "@types/mocha": "^10.0.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "19.0.0",
    "@types/react-i18next": "8.1.0",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-legacy": "^6.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "terser": "^5.14.2",
    "typescript": "5.7.3",
    "vite": "^6.1.0"
  }
}
